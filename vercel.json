{
"version":2,
"name":"Node",
"builds":[
    {"src":"app.js","use":"@vercel/node"}
],

"routes": [
    { "src": "/api/v1/user/(.*)", "dest": "/api/v1/user/$1" },
    { "src": "/api/v1/job/(.*)", "dest": "/api/v1/job/$1" },
    { "src": "/api/v1/application/(.*)", "dest": "/api/v1/application/$1" },
    { "src": "/(.*)", "dest": "/" }
  ]
}